#: ../../language/error_codes/E0037.md:1
msgid "E0037"
msgstr ""

#: ../../language/error_codes/E0037.md:3
msgid "The loop label name is never used."
msgstr "循环标签名称从未被使用。"

#: ../../language/error_codes/E0037.md:5
msgid "Erroneous example"
msgstr "错误示例"

#: ../../language/error_codes/E0037.md:7
msgid ""
"///|\n"
"fn main {\n"
"  let mut sum = 0\n"
"  let input = [1, 2, 3]\n"
"  read~: loop input[:] {\n"
"    [] => break\n"
"    [head, .. tail] => {\n"
"      sum += head\n"
"      continue tail\n"
"    }\n"
"  }\n"
"  println(sum)\n"
"}\n"
msgstr ""

#: ../../language/error_codes/E0037.md:11
msgid "Suggestion"
msgstr "建议"

#: ../../language/error_codes/E0037.md:13
msgid "Remove the unused loop label."
msgstr "移除未使用的循环标签。"

