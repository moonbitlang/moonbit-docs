#: ../../language/error_codes/E4080.md:1
msgid "E4080"
msgstr ""

#: ../../language/error_codes/E4080.md:3
msgid "Arity mismatch: incorrect number of arguments provided."
msgstr "参数数不匹配：提供的参数数不正确。"

#: ../../language/error_codes/E4080.md:5
msgid "Erroneous example"
msgstr "错误示例"

#: ../../language/error_codes/E4080.md:7 ../../language/error_codes/E4080.md:23
msgid "Function arity mismatch:"
msgstr "函数形状不匹配："

#: ../../language/error_codes/E4080.md:9
msgid ""
"fn f(x : Int, y : Double) -> Unit {\n"
"  ignore((x, y))\n"
"}\n"
"\n"
"fn main {\n"
"  f(0) // Error: This function has type (Int, Double) -> Unit, which "
"requires 2 arguments, but is given 1 argument.\n"
"}"
msgstr ""

#: ../../language/error_codes/E4080.md:13 ../../language/error_codes/E4080.md:29
msgid "Constructor arity mismatch:"
msgstr "构造函数形状不匹配："

#: ../../language/error_codes/E4080.md:15
msgid ""
"enum E {\n"
"  A(Int, Double, String)\n"
"}\n"
"\n"
"fn main {\n"
"  match A(0, 1.0) { // Error: This function has type (Int, Double, String) "
"-> E, which requires 3 arguments, but is given 2 arguments.\n"
"    A(_, _) => ... // Error: The constructor A requires 3 arguments, but is "
"given 2 arguments.\n"
"  }\n"
"}"
msgstr ""

#: ../../language/error_codes/E4080.md:19
msgid "Suggestion"
msgstr "建议"

#: ../../language/error_codes/E4080.md:21
msgid "Provide the correct number of arguments."
msgstr "提供正确数量的参数。"

#: ../../language/error_codes/E4080.md:25
msgid ""
"fn main {\n"
"  f(0, 1.0)\n"
"}"
msgstr ""

#: ../../language/error_codes/E4080.md:31
msgid ""
"fn main {\n"
"  match A(0, 1.0, \"foo\") {\n"
"    A(_, _, _) => ...\n"
"  }\n"
"}"
msgstr ""

