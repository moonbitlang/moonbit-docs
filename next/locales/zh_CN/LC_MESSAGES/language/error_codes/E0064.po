#: ../../language/error_codes/E0064.md:3
msgid "Warning name: `unannotated_toplevel_array`"
msgstr ""

#: ../../language/error_codes/E0064.md:5
msgid "Unannotated toplevel array."
msgstr ""

#: ../../language/error_codes/E0064.md:7
msgid ""
"This warning is emitted when a toplevel array literal lacks a type "
"annotation. Add an explicit type annotation to make the intended array "
"type clear."
msgstr ""

#: ../../language/error_codes/E0064.md:10
msgid "Erroneous example"
msgstr ""

#: ../../language/error_codes/E0064.md:12
msgid ""
"let xs = [1, 2, 3]\n"
"\n"
"test {\n"
"  inspect(xs[0], content=\"1\")\n"
"}\n"
"\n"
msgstr ""

#: ../../language/error_codes/E0064.md:16
msgid "Suggestion"
msgstr ""

#: ../../language/error_codes/E0064.md:18
msgid "Annotate the toplevel array:"
msgstr ""

#: ../../language/error_codes/E0064.md:20
msgid ""
"let xs : Array[Int] = [1, 2, 3]\n"
"\n"
"test {\n"
"  inspect(xs[0], content=\"1\")\n"
"}\n"
"\n"
msgstr ""

